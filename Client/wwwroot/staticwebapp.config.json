{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/*",
      "rewrite": "/index.html",
      "methods": [ "GET" ],
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "/images/*.{png,jpg,gif}", "/css/*" ]
  }
}
