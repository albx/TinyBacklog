{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/login/github",
      "rewrite": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
    {
      "route": "/*",
      "rewrite": "/index.html",
      "methods": [ "GET" ],
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "/images/*.{png,jpg,gif}", "/css/*" ]
  }
}
